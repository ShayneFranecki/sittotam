    "ipsa",
    "javascript",
    "sunt",
    "nativescript"
  ],
  "main": "./lib/LocalizedStrings.js",
  "types": "./lib/LocalizedStrings.d.ts",
  "author": "EltaSwaniawski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ShayneFranecki/sittotam.git/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
